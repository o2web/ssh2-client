{
    "name": "o2web/ssh2-client",
    "description": "Library that provides wrapper methods around SSH2 and SFTP to simplify file download/upload over SSH/SCP/SFTP. It's a https://github.com/ideaconnect/idct-sftp-client fork ",
    "type": "library",
    "keywords": [
        "sftp",
        "ssh",
        "ssh2",
        "scp",
        "idct"
    ],
    "homepage": "https://github.com/o2web/ssh2-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Bartosz PachoÅ‚ek",
            "email": "bartosz@ideaconnect.pl",
            "role": "lead"
        },
        {
            "name": "Fabricio Bedoya",
            "email": "fbedoya@o2web.ca",
            "role": "lead dev"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "ext-curl": "*",
        "psr/log": "^1.0",
        "phpunit/phpunit": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "O2Web\\Networking\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "O2Web\\Networking\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
